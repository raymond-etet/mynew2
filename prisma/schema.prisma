
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}

model Record {
  id          Int      @id @default(autoincrement())
  number      String
  publishDate DateTime
  actress     String
  tags        String
  remark      String?
}
